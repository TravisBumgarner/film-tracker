{
  "name": "film-tracker",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "db:generate": "drizzle-kit generate",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "device:register": "eas device:create",
    "build:ios:dev": "eas build -p ios --local --profile dev",
    "build:ios:internal": "eas build -p ios --local --profile internal",
    "build:ios:testflight": "eas build -p ios --local --profile testflight",
    "build:ios:production": "eas build -p ios --local --profile production",
    "build:android:dev": "eas build -p android --local --profile dev",
    "build:android:internal": "eas build -p android --local --profile internal",
    "build:android:production": "eas build -p android --local --profile production",
    "submit:ios": "eas submit -p ios --profile production",
    "submit:android": "eas submit -p android --profile production"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.14",
    "drizzle-orm": "^0.45.1",
    "expo": "~52.0.0",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.8",
    "expo-document-picker": "~13.0.1",
    "expo-file-system": "~18.0.6",
    "expo-font": "~13.0.2",
    "expo-image-picker": "~16.0.3",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.14",
    "expo-sharing": "~13.0.0",
    "expo-splash-screen": "~0.29.18",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-cloud-storage": "^2.3.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.19.13",
    "runtypes": "^6.7.0",
    "use-async-effect": "^2.2.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@biomejs/biome": "^2.3.12",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.3.12",
    "@types/uuid": "^9.0.8",
    "babel-plugin-inline-import": "^3.0.0",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.8",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "react-test-renderer": "^18.3.1",
    "typescript": "~5.3.3"
  },
  "private": true
}
